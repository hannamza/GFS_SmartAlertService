<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:height="20dp"
                android:width="20dp" />
            <solid android:color="@color/orange_100" />
            <corners
                android:radius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <size
                android:width="20dp"
                android:height="20dp" />
            <solid android:color="@color/orange_100" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="4dp"
        android:right="2dp"
        android:top="3dp"
        android:drawable="@drawable/ic_check_white"
        android:width="18dp"
        android:height="18dp"></item>
</layer-list>